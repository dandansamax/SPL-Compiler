// a testcase for structural equivalence


struct Science_fiction
{
    char name[20];
    int price;
    float weight;
};

struct Textbook
{
    char name[20];
    int price;
    float weight;
};

struct Shelf 
{
    struct Book
    {
        char name[20];
        int price;
        float weight;
    } books[100];
    float height;
};

struct Shelf s[10];

int main()
{
    struct Shelf shelf;

    int i;
    struct Textbook book1;
    struct Science_fiction book2;
    struct Book book3;
    char name[20];
    char invalid_name[19];

    // valid
    shelf.books[0] = book1;     // struct Book = struct Textbook
    shelf.books[1] = book2;     // struct Book = struct Science_fiction
    shelf.books[3].name = name; // char[20] = char[20]

    // invalid
    shelf.books[2] = i;         // struct Book = int
    shelf.books = shelf;        // struct books[100] = struct Shellf
    shelf.books[4].name = invalid_name;     // char[20] = char[19]
}